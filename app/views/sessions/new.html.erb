<h1> Login </h1> 

<% if current_user %> 
	<h3> You are logged in </h3>
<% else %>
	<%= form_tag sessions_path do %>
		<p>
			<%= label_tag :user_email %>
			<%= text_field_tag :user_email, params[:user_email] %>
		</p>
		<p>
			<%= label_tag :password %> 
			<%= password_field_tag :password %> 
		</p>

		<%= submit_tag "Login" %> 

	<% end %>
<% end %> 